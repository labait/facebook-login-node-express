<% extend('layout') %>


<div class="#upload">
  <form ref='uploadForm'
    id='uploadForm'
    action='/upload'
    method='post'
    encType="multipart/form-data">
      <input class="btn btn-default" type="file" accept="image/*" name="upload" >
      <input class="btn btn-default" type='submit' value='Upload!' />
  </form>
</div>
